: (AND)        ( x1 x2 -- x3 )                 \ ( bitwise AND TOS with NOS )
    poptmp
    tmpl tosl and
    tmph tosh and
;

MACRO: AND    ( -- )  
[F]
  QLIT1? IF 
    DEBUG3" AND"
    BYTE-SPLIT ( LO HI )
    DUP 0xFF <> IF
      tosh andi 
    ELSE DROP THEN
    DUP 0xFF <> IF 
      tosl andi 
    ELSE DROP THEN  
  ELSE
    (AND)
  THEN 
[P]
;MACRO

